//Function to ask for a new payment request 
void fetchpay(String invoiceamount) {
  
  client.beginRequest();
  String contentType = "application/x-www-form-urlencoded";
  String postData = "?POST&amount=" + invoiceamount + "&currency=btc&description=" + invoicedescription;
  client.post("/api/v1/charges");
  client.sendHeader("Content-Type", "application/x-www-form-urlencoded");
  client.sendHeader("Content-Length", postData.length());
  client.sendBasicAuth(strikeapikey, strikeapikey); 
  client.beginBody();
  client.print(postData);
  client.endRequest();
  
  response = client.responseBody();

  StaticJsonBuffer<2000> jsonBuffer;
  JsonObject& root = jsonBuffer.parseObject(response);

  String paymentpaid = root["paid"];
  PAYMENTPAID = paymentpaid;
  String paymentreq = root["payment_request"];
  PAYMENT = paymentreq;
  response = "";
}


// QR maker function
void qrmmaker(String xxx){

int str_len = xxx.length() + 1; 
char xxxx[str_len];
xxx.toCharArray(xxxx, str_len);

    // Start time
    uint32_t dt = millis();
  
    // Create the QR code
    QRCode qrcode;
    uint8_t qrcodeData[qrcode_getBufferSize(11)];
    qrcode_initText(&qrcode, qrcodeData, 11, 0, xxxx);
  
    // Delta time
    dt = millis() - dt;
    Serial.print("QR Code Generation Time: ");
    Serial.print(dt);
    Serial.print("\n");

    int une = 0;
    
    line = "";

    for (uint8_t y = 0; y < qrcode.size; y++) {

        // Each horizontal module
        for (uint8_t x = 0; x < qrcode.size; x++) {
          line += (qrcode_getModule(&qrcode, x, y) ? "000": "111");
        }
        line += "0";
        for (uint8_t x = 0; x < qrcode.size; x++) {
          line += (qrcode_getModule(&qrcode, x, y) ? "000": "111");
        }
        line += "0";
        for (uint8_t x = 0; x < qrcode.size; x++) {
          line += (qrcode_getModule(&qrcode, x, y) ? "000": "111");
        }
        line += "0";    
    }
}
